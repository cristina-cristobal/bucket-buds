<%= g.fields_for :likes, @goal.likes.build do |l| %>
  <%= l.label :description %>
  <%= l.text_area :description %><br>
  <%= l.label :timeline, "Complete By" %>
  <%= l.date_select(:timeline, :order => [:month, :day, :year]) %><br>
  <%= l.hidden_field :user_id, value: session[:user_id] %>
<% end %>
